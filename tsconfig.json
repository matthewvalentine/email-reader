{
  // NOTE: These options are overridden in webpack.config.js.
  "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "moduleResolution": "node",
      "sourceMap": true,
      "strict": true,
      "removeComments": false,
      "jsx": "react",
      "outDir": "dist",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true
  }
}
